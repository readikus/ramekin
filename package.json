{
  "name": "ramekin",
  "version": "0.1.4",
  "description": "An open source, real time trend detection library. This project uses machine learning to detect trends in text (i.e. news stories) over time.",
  "main": "ramekin.js",
  "dependencies": {
    "jest": "^24.9.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "natural": "^0.6.3"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2"
  },
  "homepage": "https://github.com/readikus/ramekin",
  "author": "Ian Read <ianharveyread@gmail.com>",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/readikus/ramekin.git"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint --ext .js .",
    "lint:fix": "eslint --fix --ext .js .",
    "test:watch": "jest --watch"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ]
  }
